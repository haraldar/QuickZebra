<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net6.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <GeneratePackageOnBuild>True</GeneratePackageOnBuild>
    <Product>QuickZebra</Product>
    <Company>Harald Asmus</Company>
    <Authors>Harald Asmus</Authors>
    <Version>1.0.0</Version>
    <PackageId>QuickZebra</PackageId>
    <Description>Quickly create a Zebra label and send the generated ZPL code to Labelary's API to get a preview.</Description>
    <Copyright>MIT</Copyright>
    <PackageProjectUrl>https://github.com/haraldar/QuickZebra</PackageProjectUrl>
    <RepositoryUrl>https://github.com/haraldar/QuickZebra</RepositoryUrl>
    <PackageIcon>icon_qz.jpg</PackageIcon>
    <ApplicationIcon>icon_qz.ico</ApplicationIcon>
    <RepositoryType>git</RepositoryType>
  </PropertyGroup>

  <ItemGroup>
    <Content Include="icon_qz.ico" />
  </ItemGroup>

  <ItemGroup>
    <None Include="..\..\..\icon_qz.jpg">
      <Pack>True</Pack>
      <PackagePath>\</PackagePath>
    </None>
  </ItemGroup>

</Project>
